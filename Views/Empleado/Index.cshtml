            <div class="block-header">
                <h2>
                    Empleados
                </h2>
            </div>
            <!-- Basic Examples -->
            <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="card">
                        <div class="body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover js-basic-example dataTable" id="DatoEmpleado">
                                    <thead>
                                        <tr>
                                            <th>Cedula</th>
                                            <th>Nombre</th>
                                            <th>Primer Apellido</th>
                                            <th>Segundo Apellido</th>
                                            <th>Correo</th>
                                            <th>Habilitado</th>
                                            <th>Modificar</th>
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th>Cedula</th>
                                            <th>Nombre</th>
                                            <th>Primer Apellido</th>
                                            <th>Segundo Apellido</th>
                                            <th>Correo</th>
                                            <th>Habilitado</th>
                                            <th>Modificar</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #END# Basic Examples -->
        </div>

        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">×</button>
                        <h4 class="modal-title" id="myModalLabel">Empleado</h4>
                    </div>
                    <div class="modal-body">
                        <form>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-6">
                                        <label for="Cedula">Cedula</label>
                                        <input type="text" class="form-control" id="Cedula" placeholder="Cedula" />
                                    </div>
                                    <div class="col-xs-6">
                                        <label for="Nombre">Nombre</label>
                                        <input type="text" class="form-control" id="Nombre" placeholder="Nombre" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-6">
                                        <label for="Primer_Apellido">Primer Apellido</label>
                                        <input type="text" class="form-control" id="Primer_Apellido" placeholder="Primer Apellido" />
                                    </div>
                                    <div class="col-xs-6">
                                        <label for="Segundo_Apellido">Segundo Apellido</label>
                                        <input type="text" class="form-control" id="Segundo_Apellido" placeholder="Segundo Apellido" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-6">
                                        <label for="Correo">Correo</label>
                                        <input type="email" class="form-control" id="Correo" placeholder="Correo" />
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" id="btnAdd" onclick="return Add();">Agregar</button>
                        <button type="button" class="btn btn-primary" id="btnUpdate" style="display:none;" onclick="Update();">Actualizar</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal" onclick="return clearTextBox();">Cerrar</button>
                    </div>
                </div>
            </div>
        </div>
    </section>

